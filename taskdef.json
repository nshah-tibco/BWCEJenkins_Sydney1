{
    "family": "launch-test-app1",
    "containerDefinitions": [
        {
            "image": "750037626691.dkr.ecr.ap-southeast-2.amazonaws.com/bwce-app1:latest",
            "name": "bwce-test-app1",
            "cpu": 10,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 9012,
                    "hostPort": 9012
                }
            ],
            "environment": [
                {
                    "name": "BW_LOGLEVEL",
                    "value": "ERROR"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "bwce-app1-log",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "bwce"
                }
	    }
        }
    ]
}
